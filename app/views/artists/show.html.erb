<h1><%= @artist.name %></h1>

<img src="<%=@artist.image%>" alt="<%=@artist.name%>" style="width:200px;height:200px;">


<p>Songs: <%= @songs.count %></p>
<ul>
    <% @songs.each do |song| %>
        <li><%= link_to song.title, song %></li>
    <% end %>
</ul>
<%= button_to "View All Artists", artists_path, method: :get %>